{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/services.js",
    ".next/types/**/*.ts"
, "pages/shortUrl/[...shortenUrl].js", "src/components/ShortLinkList.jsx", "src/components/ShortLinkForm.jsx", "src/components/Ranking.jsx", "src/components/Button.jsx", "src/components/Input.jsx", "src/components/Header.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
